{
  "name": "vue-gtag",
  "description": "Global Site Tag (gtag.js) plugin for Vue",
  "type": "module",
  "version": "2.1.0",
  "author": {
    "name": "Matteo Gabriele",
    "email": "m.gabriele.dev@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatteoGabriele/vue-gtag"
  },
  "scripts": {
    "dev": "vite build --watch",
    "fix": "biome check --write",
    "build": "tsc --noEmit && vite build",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:once": "vitest run",
    "prepublishOnly": "yarn check && yarn test:once && yarn build"
  },
  "keywords": [
    "google",
    "google analytics",
    "tracking",
    "google tracking",
    "vue-analytics",
    "vue-gtag",
    "gtag",
    "gtag.js",
    "global site tag",
    "vue",
    "vuejs"
  ],
  "main": "./dist/vue-gtag.cjs.js",
  "module": "./dist/vue-gtag.es.js",
  "unpkg": "./dist/vue-gtag.umd.js",
  "jsdelivr": "./dist/vue-gtag.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/MatteoGabriele/vue-gtag/issues"
  },
  "homepage": "https://github.com/MatteoGabriele/vue-gtag#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.5",
    "flush-promises": "^1.0.2",
    "jsdom": "^26.0.0",
    "mockdate": "^3.0.5",
    "terser": "^5.39.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5",
    "vue": "^3.0.0",
    "vue-router": "^4.5.0"
  }
}
